@model List<Tuple<string, string>>

<ol class="breadcrumb">

    @if (Model != null)
    {
        for (var i = 0; i < Model.Count; i++)
        {
            <li>
                <a href="@Model[i].Item1">
                    @if (i == Model.Count - 1)
                    {
                        <strong>@Model[i].Item2</strong>
                    }
                    else
                    {
                        @Model[i].Item2
                    }
                </a>
            </li>
        }

    }
</ol>
